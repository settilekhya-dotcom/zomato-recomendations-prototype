{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/frontend/$1"
        },
        {
            "source": "/(.*)",
            "destination": "main.py"
        }
    ]
}